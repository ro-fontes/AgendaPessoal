<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.listapersonagem">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/agenda_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ListaPersonagem">
        <activity android:name=".ui.activities.FormularioPersonagemActivity"/>
        <activity android:name=".ui.activities.ListaPersonagemActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
</manifest>